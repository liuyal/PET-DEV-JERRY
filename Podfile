  pod 'Spring', :git => 'https://github.com/MengTo/Spring.git'

    target ‘PETTests’ do
        inherit! :search_paths
	      use_frameworks!
    end

    target ‘PETUITests’ do
        inherit! :search_paths
	      use_frameworks!
    end

end
